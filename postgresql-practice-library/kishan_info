DROP TABLE IF EXISTS kishan_info;
CREATE DATABASE kishan_info;
CREATE TABLE kishan_info(
name VARCHAR(30),
F_ID INT,
village_name VARCHAR(50),
age INT
);
INSERT INTO kishan_info(name,F_ID,village_name,age)
