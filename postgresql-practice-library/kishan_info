('niral paswan',233,'sidha',30),
('aditya mahto',262,'tipura',25),
('raju  kumar',255,'khadah tola',21),
('sunny kumar',215,'lakhan tola',40)
SELECT*FROM kishan_info;

UPDATE  kishan_info
SET name = 'aditya kushwaha'
WHERE name = 'aditya kumar'

DELETE FROM kishan_info
WHERE F_ID = 22
